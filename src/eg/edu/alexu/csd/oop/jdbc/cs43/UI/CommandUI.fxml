<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane prefHeight="509.0" prefWidth="410.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="eg.edu.alexu.csd.oop.jdbc.cs43.UI.UIController">
   <center>
      <AnchorPane prefHeight="343.0" prefWidth="394.0" BorderPane.alignment="CENTER">
         <children>
            <TextField fx:id="input" layoutX="9.0" layoutY="74.0" onKeyPressed="#enterCommand" prefHeight="25.0" prefWidth="377.0" promptText="Enter Command Here" />
            <ScrollPane layoutX="12.0" layoutY="151.0" prefHeight="131.0" prefViewportHeight="200.0" prefViewportWidth="200.0" prefWidth="371.0">
               <content>
                  <TextArea fx:id="output" layoutX="99.0" layoutY="73.0" prefHeight="130.0" prefWidth="370.0" />
               </content>
            </ScrollPane>
            <Button fx:id="path" layoutX="120.0" layoutY="30.0" mnemonicParsing="false" onAction="#fileChooseOpen" prefHeight="25.0" prefWidth="155.0" text="choose database path" />
            <TableView fx:id="table" layoutX="15.0" layoutY="295.0" prefHeight="200.0" prefWidth="368.0" />
            <Button fx:id="addBatch" layoutX="23.0" layoutY="111.0" mnemonicParsing="false" onAction="#addBatch" prefHeight="25.0" prefWidth="90.0" text="add batch" />
            <Button fx:id="executeBatch" layoutX="153.0" layoutY="111.0" mnemonicParsing="false" onAction="#executeBatch" prefHeight="25.0" prefWidth="90.0" text="execute batch" />
            <Button fx:id="clearBatch" layoutX="275.0" layoutY="111.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="90.0" text="clear batch" />
         </children>
      </AnchorPane>
   </center>
</BorderPane>
